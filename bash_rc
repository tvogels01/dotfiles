set -o vi

alias la='ls -A'
alias ll='ls -Al'
alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -i'
alias h='history'

export SBT_OPTS="-XX:+CMSClassUnloadingEnabled"

export GREP_COLOR="auto"

# export PS1='\h:\W \u\$ '
export PS1='\w$(__git_ps1 " (%s)") \$ '

# HOMEBREW_GITHUB_API_TOKEN
if [ -r ~/.private/keys ]; then
  source ~/.private/keys
fi
source ~/etc/psql_completion.sh

